<%- include('../partials/header') %>

<h1 class="title-text">User List Page</h1>
<% users.forEach(u => { %>
  <a href="/users/<%= u._id %>">
    <div class="card">
        <img height="50" id="avatarPhoto" src="<%= u.avatar ? u.avatar : 'https://i.imgur.com/DuUZpapt.jpg' %>" alt=""> 
        <%= u.name %></a> - Joined on <%= u.createdAt.toLocaleString().split(',')[0] %>
      </div>
    </div><br>
<% }) %>

  <%- include('../partials/footer') %>
